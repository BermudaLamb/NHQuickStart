<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="NHQuickStart" namespace="NHQuickStart.Models.Production" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Productmodel" table="ProductModel" schema="Production" lazy="true" >
		<id name="Productmodelid">
			<column name="ProductModelID" sql-type="int" not-null="true" />
			<generator class="identity" />
		</id>

		<property name="Name">
			<column name="Name" sql-type="nvarchar" not-null="true" />
		</property>

		<property name="Catalogdescription">
			<column name="CatalogDescription" sql-type="xml" not-null="false" />
		</property>

		<property name="Instructions">
			<column name="Instructions" sql-type="xml" not-null="false" />
		</property>

		<property name="Rowguid">
			<column name="rowguid" sql-type="uniqueidentifier" not-null="true" />
		</property>

		<property name="Modifieddate">
			<column name="ModifiedDate" sql-type="datetime" not-null="true" />
		</property>

  </class>
</hibernate-mapping>
