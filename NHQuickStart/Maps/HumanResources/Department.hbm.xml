<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="NHQuickStart" namespace="NHQuickStart.Models.HumanResources" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Department" table="Department" schema="HumanResources" lazy="true" >
		<id name="Departmentid">
			<column name="DepartmentID" sql-type="smallint" not-null="true" />
			<generator class="identity" />
		</id>

		<property name="Name">
			<column name="Name" sql-type="nvarchar" not-null="true" />
		</property>

		<property name="Groupname">
			<column name="GroupName" sql-type="nvarchar" not-null="true" />
		</property>

		<property name="Modifieddate">
			<column name="ModifiedDate" sql-type="datetime" not-null="true" />
		</property>

  </class>
</hibernate-mapping>
